{
  "name": "@foxglove/schemas",
  "version": "0.6.1",
  "description": "Foxglove-defined message schemas for ROS, Protobuf, and JSON",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/foxglove/schemas.git"
  },
  "author": {
    "name": "Foxglove Technologies",
    "email": "support@foxglove.dev"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/schemas/typescript/index.js",
      "require": "./dist/cjs/schemas/typescript/index.js"
    },
    "./jsonschema": {
      "import": "./dist/esm/schemas/jsonschema/index.js",
      "require": "./dist/cjs/schemas/jsonschema/index.js"
    },
    "./internal": {
      "import": "./dist/esm/src/index.js",
      "require": "./dist/cjs/src/index.js"
    }
  },
  "scripts": {
    "prepack": "tsc -b tsconfig.json tsconfig.cjs.json && yarn update-generated-files",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint:ci": "eslint --report-unused-disable-directives .",
    "lint": "eslint --report-unused-disable-directives --fix .",
    "test": "jest",
    "update-generated-files": "ts-node --files --project tsconfig.cjs.json scripts/updateGeneratedFiles --out-dir schemas --ros-out-dir ros_foxglove_msgs"
  },
  "devDependencies": {
    "@foxglove/eslint-plugin": "0.20.0",
    "@foxglove/rosmsg": "^3.1.0",
    "@foxglove/tsconfig": "1.1.0",
    "@types/jest": "29.0.1",
    "@types/node": "18.7.16",
    "@types/rimraf": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "ajv": "^8.11.0",
    "commander": "^9.4.0",
    "eslint": "8.23.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-es": "4.1.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.0.4",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.0.3",
    "prettier": "2.7.1",
    "protobufjs": "^7.1.1",
    "rimraf": "^3.0.2",
    "ts-jest": "29.0.0",
    "ts-node": "10.9.1",
    "typescript": "4.8.3"
  },
  "dependencies": {
    "@foxglove/rosmsg-msgs-common": "^2.0.0",
    "tslib": "^2"
  }
}
